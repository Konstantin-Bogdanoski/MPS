; multi-segment executable file template.

data segment
; add your data here!
pkey db "press any key...$"
msg1 db 10,13,"Vnesi go x: $"
ends

stack segment
dw   128  dup(0)
ends

code segment
start:
; set segment registers:
mov ax, data
mov ds, ax
mov es, ax

; add your code here
mov ax, 12d
;x mod y == x and (y-1)
and ax, 3


mov ax, 4c00h ; exit to operating system.
int 21h
ends

end start ; set entry point and stop the assembler.


